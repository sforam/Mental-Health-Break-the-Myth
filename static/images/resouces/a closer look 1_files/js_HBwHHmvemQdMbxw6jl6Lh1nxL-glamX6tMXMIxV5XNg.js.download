// Handles printing
function printIt(object) {
    function setHtmlElementActive(object, boolStatus, querySelector) {
        if (detectIE() === false) {
            if (boolStatus) {
                $(object).attr('aria-pressed', 'true');
            } else {
                $(object).removeAttr('aria-pressed');
            }
        } else {
            var obj = document.querySelectorAll(querySelector);
            if (boolStatus) {
                obj[0].setAttribute('aria-pressed', true);
            } else {
                obj[0].setAttribute('aria-pressed', false);
                obj[0].removeAttribute('aria-pressed');
            }
        }
    }

    window.onbeforeprint = setHtmlElementActive(object, true, 'button.print-button'); //This will be called before the user prints.
    window.print();
    window.onafterprint = setHtmlElementActive(object, false, 'button.print-button'); //This will be called after the user prints
}

