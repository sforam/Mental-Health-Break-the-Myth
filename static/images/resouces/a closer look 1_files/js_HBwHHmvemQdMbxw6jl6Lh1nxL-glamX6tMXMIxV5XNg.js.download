                    if (options.length || (text && text.length)) {
                        var subItems = items[questionType] || [];
                        subItems.push({
                            QuestionId: questionId,
                            Options: options,
                            Text: text,
                        });
                        items[questionType] = subItems;
                    }
                }
            });

            // Check if the captcha is displayed for this feedback type
            var hasCaptcha = Modals.getFeedbackModal().find('.feedback-type-' + feedbackType + '.captcha-question').length;

            // Build data
            var feedbackData = {
                FeedbackId: Modals.getFeedbackModal().find('.feedback-modal-feedback-id.feedback-type-' + feedbackType).val(),
                PageId: getPageId(),
                SelectedItems: items,
                CaptchaAnswer: Modals.getFeedbackModal().find('.feedback-type-' + feedbackType + ' .feedback-modal-captcha').val() || ' ',
                hasCaptcha: hasCaptcha,
                CaptchaQuestion: CaptchaQuestion,
                FeedbackType: feedbackType,
                realFeedbackType: Modals.getFeedbackModal().find('.realFeedbackType-' + feedbackType).val(),
