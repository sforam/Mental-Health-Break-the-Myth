    function loadCaptcha(captcha) {
        function updateCaptcha(captcha) {
            CaptchaQuestion = captcha;
            Modals.getFeedbackModal().find('.feedback-modal-captcha-question').text(captcha);
            Modals.getFeedbackModal().find('.feedback-modal-captcha').val('');
        }

        if (!captcha) {
            var captchaHandler = new CaptchaHandler(Constants.Captcha.Url);
            captchaHandler.load(updateCaptcha);
        } else {
            updateCaptcha(captcha);
        }
    }

    // Loads a new anti-forgery token
