$(document).ready(function ($) {
  if (document.getElementsByClassName("header__crisis-area-arrow-button") && document.getElementsByClassName("header__crisis-area-alert")) {
      // Determine if this is a next button or a prev button
      var isNext = $(this).is(".header__crisis-area-arrow-button--next");

      // Find currently displayed alert and the next/prev sequential alert (wrapping around if needed)
      var alert = $(".header__crisis-area-alert:not(.header__crisis-area-alert--hidden)");
