// Calculates the height of our top navigation, accounting for various elements that hide/show
function getTopNavigationHeight() {
    // Start with the height of the header
    var navHeight = $('#header').height();

    // Add height of the service providers page nav, if it exists
    var serviceProvidersNav = $('.alphabetical-navigation');
    if (serviceProvidersNav.length) {
        navHeight = navHeight + serviceProvidersNav.height();
