    return openInnerMenu.length !== 0;
};

// Handle tabbing in the Desktop inner menu
//
// There is an accessibility requirement that while in desktop mode, tabbing form the last item in the inner menu (level 3) go to the
// feature box and not the next menu item (level 2).
// NOTE: we assume there is always at least one feature box present.
var handleTabbingInDesktopInnerMenu = function () {
    $(".header__menu-item--type-desktop-level-3").off('keydown').on('keydown', function (e) {
        var lastMenuItem = $(".header__desktop-menu-inner-panel-menu-item-list").find('.header__menu-item--type-desktop-level-3:last');
        var isLastMenuItem = jQuery.inArray(this, lastMenuItem) !== -1;
        var keyCode = e.keyCode || e.which;

        // Tab event
        if (keyCode === Constants.KeyCode.Tab) {
            // If the inner menu (L3) is open then:
            // Tabbing from the last item in the inner menu goes to the first feature box
            if (!e.shiftKey && isLastMenuItem) {
                e.preventDefault();
                e.stopImmediatePropagation();
                $(this).closest(".header__desktop-menu-panel").find(".header__related-content-item-list").find(".feature-box__button:first").focus();
            }
