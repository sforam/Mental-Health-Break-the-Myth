// Misc ---------------------------------------------------------------------------------------

// updateHeaderSpacerHeight()
//
// Ensures our header spacer is the correct height at all times.
//
// Our header is sticky (via `position: fixed`) and does not contribute to the normal document
// flow. This causes content near the top of the page to be hidden behind the header. To
// resolve this, we dynamically add a "spacer" element just before our header that is
// automatically sized to match the height of our header - thus, pushing all elements down so
// that none are behind the header. This function creates that spacer and keeps its height up to
// date.
//
var updateHeaderSpacerHeight = function () {
    // Get elements
    var header = $(".header");
    var spacer = $(".header-spacer");

    // Keep its height updated
    var update;

    update = function () {
        var targetHeight = header.height();

        // GovCMS: Need to also add `padding-top` from our body, to account for the Drupal admin toolbar
        /*var bodyPaddingTop = $("body").css("padding-top");
        var bodyPaddingTopNum = parseInt(bodyPaddingTop);
        if (!isNaN(bodyPaddingTopNum)) {
            targetHeight += bodyPaddingTopNum;
        }*/

        // Also need to update the `top` of our header to match our body's
        var bodyPaddingTop = $("body").css("padding-top");
        header.css("top", bodyPaddingTop);

        spacer.height(targetHeight);
        requestAnimationFrame(update);
    };
    update();
};

