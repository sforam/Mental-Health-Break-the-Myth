        });

        // Setup textarea options
        Modals.getFeedbackModal().find('.textarea-option').each(function () {
            var textareaContainer = $(this).parent().next();

            // When we're changed
            $(this).off('change').on('change', function () {
                if ($(this).is(':checked')) {
                    textareaContainer.removeClass('disabled');
                    textareaContainer.find('textarea').prop('disabled', false);
                } else {
                    textareaContainer.addClass('disabled');
                    textareaContainer.find('textarea').prop('disabled', true);
                }
            }).triggerHandler('change');
        });

        // Set state
        setState(State.Questions);

        // Set question index
        setCurrentQuestionIndex(1);

        // Update feedback type options to show the correct number of questions
        if (needToUpdateFeedbackTypeOptions) {

            //Site Label
            var siteLabel = Modals.getFeedbackModal().find('.feedback-modal-feedback-type-site-label');
            var siteAppend = ' (' + getNumQuestions('site') + ' questions)';
            siteLabel.append(siteAppend);
            siteLabel.attr('aria-label', siteLabel.text() + '. Complete at least one question');

            //Page Label
            var pageLabel = Modals.getFeedbackModal().find('.feedback-modal-feedback-type-page-label');
            var pageAppend = ' (' + getNumQuestions('page') + ' questions)';
            pageLabel.append(pageAppend);
            pageLabel.attr('aria-label', pageLabel.text() + '. Complete at least one question');


            needToUpdateFeedbackTypeOptions = false;
