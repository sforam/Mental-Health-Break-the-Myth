// Validates email format
EmailFormHandler.prototype.validateEmail = function(email) {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|('.+'))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
};

// Validates email form
EmailFormHandler.prototype.validateEmailForm = function() {
    var email = this.emailInput.val();

    if (!email || email === '') {
        this.showEmailError('Email address missing. Please enter address to continue.', false);
        return false;
    }
    else if (email && email !== '' && !this.validateEmail(email)) {
        this.showEmailError('Invalid email address. Please check address to continue.', false);
        return false;
    }
    else if (this.captchaInput.val() === '') {
        this.showEmailError('Please fill in a correct captcha answer.', true);
        return false;
    }

    return true;
};

EmailFormHandler.prototype.sendEmail = function (emailContent, captchaAnswer) {
    // This duplicate check is here on purpose - in case user changes email field together with captcha
    var validForm = this.validateEmailForm();
    var emailInput = this.emailInput.val();
    var headers = {};
    headers[Constants.AntiForgeryToken.Header] = this.antiForgeryInput.val();
    this.submitButtonLoader.removeClass('stopped');

    if (validForm) {
        $.ajax({
                url: '/dmhgapi/Email/SendEmail',
                method: 'POST',
                headers: headers,
                data: '{ "data":' + emailContent + ', "captchaAnswer": "' + captchaAnswer + '", "toAddress": "' + emailInput + '"}',
                dataType: "json",
                contentType: 'application/json; charset=utf-8'
