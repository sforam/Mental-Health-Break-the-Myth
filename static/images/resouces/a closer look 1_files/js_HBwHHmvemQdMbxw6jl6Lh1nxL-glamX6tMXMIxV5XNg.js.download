// closeMobletMenuInnerPanels(goingBackward, goingForward)
//
// Closes inner panels inside the moblet menu.
//
// Params:
// - `goingBackward`: Are we moving backwards? (Adds a hide animation to the currently showing panel).
// - `goingForward`: Are we moving forwards? (Adds a hide animation to the currently showing panel).
//
// Notes:
// - Both `goingBackward` and `goingForward` can be false.
//
var closeMobletMenuInnerPanels = function (goingBackward, goingForward) {
    // Handle going backward/forward
    if (goingBackward || goingForward) {
        if (goingBackward) {
            $(".header__moblet-menu-panel--show")
                .addClass("header__moblet-menu-panel--hide")
                .addClass("header__moblet-menu-panel--hide-backward");
        }

        if (goingForward) {
            $(".header__moblet-menu-panel--show")
                .addClass("header__moblet-menu-panel--hide")
                .addClass("header__moblet-menu-panel--hide-forward");
        }

        setTimeout(function () {
            $(".header__moblet-menu-panel--hide")
                .removeClass("header__moblet-menu-panel--hide")
                .removeClass("header__moblet-menu-panel--hide-forward")
                .removeClass("header__moblet-menu-panel--hide-backward");
        }, mobletMenuPanelAnimationDuration);
    }

    // Close panels
    $(".header__moblet-menu-panel")
        .removeClass("header__moblet-menu-panel--show")
        .removeClass("header__moblet-menu-panel--show-forward")
        .removeClass("header__moblet-menu-panel--show-backward");

