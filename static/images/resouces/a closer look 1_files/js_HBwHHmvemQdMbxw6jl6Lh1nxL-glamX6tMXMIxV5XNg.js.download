    });
};

// Handle tabbing from feature boxes in the Desktop menu
var handleTabbingInDesktopMenuFeatureBox = function () {
    $(".header__related-content--type-desktop").find(".header__related-content-item-list").find(".feature-box__button").off('keydown').on('keydown', function (e) {
        var currentMenuPanel  = $(this).closest(".header__desktop-menu-panel");
        var currentMenuItemLi = currentMenuPanel.find(".header__desktop-menu-inner-panel--show").closest(".header__desktop-menu-panel-menu-item"); // Find which menu item we are by finding which one is showing the inner menu panel
        var currentMenuItem   = currentMenuItemLi.find(".header__menu-item--type-desktop-level-2");
        var lastInnerMenuItem = currentMenuPanel.find(".header__desktop-menu-inner-panel--show").find(".header__menu-item--type-desktop-level-3:last");
        var lastMenuItem    = currentMenuPanel.find('.header__menu-item--type-desktop-level-2:last');
        var isLastMenuItem  = currentMenuItem.is(lastMenuItem);
        var isInnerMenuOpen = isDesktopInnerMenuOpen(currentMenuPanel);
        var firstFeatureBoxItem   = $(this).closest(".header__related-content-item-list").find(".feature-box__button:first");
        var isFirstFeatureBoxItem = $(this).is(firstFeatureBoxItem);
        var lastFeatureBoxItem    = $(this).closest(".header__related-content-item-list").find(".feature-box__button:last");
        var isLastFeatureBoxItem  = $(this).is(lastFeatureBoxItem);
        var keyCode = e.keyCode || e.which;

        // Tab event
        if (keyCode === Constants.KeyCode.Tab) {
            // If the inner menu (L3) is open then:
            // Tabbing from the last feature box item goes to the next menu item (L2) if there is one
            if (!e.shiftKey && isLastFeatureBoxItem && !isLastMenuItem && isInnerMenuOpen) {
