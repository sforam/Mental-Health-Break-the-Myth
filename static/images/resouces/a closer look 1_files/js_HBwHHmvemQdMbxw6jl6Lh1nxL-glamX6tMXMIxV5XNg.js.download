// showMobletMenuInnerPanel(panel, goingBackward, goingForward)
//
// Shows an inner panel inside the moblet menu.
//
// Params:
// - `panel`: jQuery object referencing the panel to be shown.
// - `goingBackward`: Are we moving backwards? (Adds a show animation to the panel).
// - `goingForward`: Are we moving forwards? (Adds a show animation to the panel).
//
var showMobletMenuInnerPanel = function (panel, goingBackward, goingForward) {
    // Show panel
    panel.addClass("header__moblet-menu-panel--show");

    // Handle going backward/forward
    if (goingBackward || goingForward) {
        // Going backward/forward - add classes for animations
        if (goingBackward) {
            panel.addClass("header__moblet-menu-panel--show-backward");
        }

        if (goingForward) {
            panel.addClass("header__moblet-menu-panel--show-forward");
        }

        // Adjust moblet content spacer - but not for the first panel
        // This is because there's no related content on the first panel, so we don't need the
        // spacer to be the right height for it, and this avoid a weird diagonal animation
        if (panel.data("menu-item-id") !== "") {
            // We also want to avoid the height animation when moving _from_ the first panel
            // In those cases we use `min-height` to avoid it being transitioned
            if ($(".header__moblet-menu-panel--hide").data("menu-item-id") === "") {
                // Moving from first panel

                // Adjust moblet content spacer WITHOUT a transition
                $(".header__moblet-menu-content-spacer")
                    .css("transition", "none")
                    .height(panel.height());
                setTimeout(function () {
                    $(".header__moblet-menu-content-spacer").css("transition", "");
                }, 100);
            } else {
                // Not moving from first panel

                // Adjust moblet content spacer with a transition
                $(".header__moblet-menu-content-spacer").height(panel.height());
            }
        }
    } else {
        // Not going backward/forward - we want to _instantly_ change, no animations!

        // Adjust moblet content spacer WITHOUT a transition
        $(".header__moblet-menu-content-spacer")
            .css("transition", "none")
            .height(panel.height());
        setTimeout(function () {
            $(".header__moblet-menu-content-spacer").css("transition", "");
        }, 100);
    }
};

