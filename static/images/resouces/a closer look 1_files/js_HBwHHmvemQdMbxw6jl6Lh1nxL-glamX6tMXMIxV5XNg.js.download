    this.captchaQuestionTextElement.text(question);

    this.emailForm.addClass('loaded');
};

EmailFormHandler.prototype.submitClickHandler = function(e) {
    e.preventDefault();
    this.hideEmailError();
    var validForm = this.validateEmailForm();

    if (validForm) {
        this.sendEmail(this.getJSONData(), this.captchaInput.val());
    }

    return false;
