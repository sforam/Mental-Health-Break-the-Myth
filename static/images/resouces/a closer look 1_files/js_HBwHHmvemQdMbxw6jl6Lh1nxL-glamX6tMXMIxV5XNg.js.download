            else if (!e.shiftKey && isLastFeatureBoxItem && (!isInnerMenuOpen || (isInnerMenuOpen && isLastMenuItem))) {
                closeDesktopMenu();
            }
            // If the inner menu (L3) is open then:
            // Shift tabbing from first feature box item goes to the last menu item in the inner menu (L3)
            else if (e.shiftKey && isFirstFeatureBoxItem && isInnerMenuOpen) {
