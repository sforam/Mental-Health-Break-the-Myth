    return navHeight + 20;
}

// Handles the "jump-to-content" link
function JumpToContentHandler(jumpToContentId) {
    // Determine the ID of the element that we want to jump to
    if (!jumpToContentId) {
        // By default we jump to #content-of-page
        jumpToContentId = 'content-of-page';

        // However, if there is an element with the class `content-of-page`, use its ID instead (if it has one)
        var pageContentId = $('.content-of-page').attr('id');
        if (pageContentId) {
            jumpToContentId = pageContentId;
        }
    }

    // Find our elements
    this.jumpToElement = $('#' + jumpToContentId);
    this.jumpToContentLink = $('#jump-to-content-link');
    this.jumpToContentLink.attr('href', '#' + jumpToContentId);

    // Init listener
    this.initBinding = function () {
        this.jumpToContentLink
            .off('click')
            .on('click', $.proxy(this.jumpToContentClickHandler, this));

        this.jumpToContentLink
            .off('keypress')
            .on('keypress', $.proxy(this.jumpToContentKeyPressHandler, this));
    };

    // Handle when we press a key on our link
    this.jumpToContentKeyPressHandler = function (e) {
        // Spacebar
        if (e.which === 32) {
            e.preventDefault();
            e.stopImmediatePropagation();

            $(e.target).click();
        }
    };

    // Handle when our link is clicked
    this.jumpToContentClickHandler = function (e) {
        e.preventDefault();
        e.stopImmediatePropagation();

        // Scroll
        accSmoothScrollToElement(this.jumpToElement);
    };
}

// Handles enabling/disabling the ability to scroll the page
function togglePageScrollingIfNeeded(forceDisable, forceEnable) {
    var body = $('body');

    var shouldDisable = isNavVisible() || body.is('.modal-open');

    if (forceDisable) {
        shouldDisable = true;
    }

    if (forceEnable) {
        shouldDisable = false;
    }

    if (shouldDisable) {
        // Nav is shown or a modal is showing - disable page scrolling (if it's not already)
        if (body.css('position') !== 'fixed') {
            togglePageScrollingIfNeeded.scrollTop = $(window).scrollTop();

            var cssArgs = {
                overflow: 'hidden',
                left: '0px',
                right: '0px',
                bottom: '0px',
                top: -togglePageScrollingIfNeeded.scrollTop + 'px',
                position: 'fixed'
            };

            //only for IE 11
            if (detectIE() === 11) {
                cssArgs["z-index"] = 1000;
            }
            body.css(cssArgs);
            body.scrollTop(0);
        }
    } else {
        // Nav is hidden and a modal isn't showing - enable page scrolling
        body.removeAttr('style');
        $(window).scrollTop(togglePageScrollingIfNeeded.scrollTop);
    }
}

