                    // check if can be redisplayed
                    if (redisplayHours > 0 || redisplayDays > 0) {
                        var redisplayDate = surveyDismissDate.clone();
                        redisplayDate.add(redisplayDays, 'days');
                        redisplayDate.add(redisplayHours, 'hours');

                        if (moment(Date.now()) > redisplayDate) {
                            showBanner = true;
                        }
                    }
                }
            }
