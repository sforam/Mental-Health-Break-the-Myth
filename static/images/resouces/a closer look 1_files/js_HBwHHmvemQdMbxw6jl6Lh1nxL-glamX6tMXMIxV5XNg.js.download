    if (invalidCaptcha) {
        // Invalid captcha
        this.captchaInput.addClass('error');
        this.emailInput.removeClass('error');
        this.captchaInput.focus();
    } else {
        // Invalid email
        this.emailInput.addClass('error');
        this.captchaInput.removeClass('error');
        this.emailInput.focus();
