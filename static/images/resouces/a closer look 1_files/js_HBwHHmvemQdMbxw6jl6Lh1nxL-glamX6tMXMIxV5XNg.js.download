// This function will resize the boxes depending on the text inside and their maximum width, otherwise,
// flex boxes will take the max width of the parent element, and the text will wrap, this might leave too much space on the right
function resizeTextBox(textElement) {
    var width = textElement.width();
    var height = textElement.height();

    textElement.css('word-wrap', 'break-word');
    textElement.css('word-break', 'break-all');
    while (width > 0 && textElement.height() === height) {
        textElement.width(--width);
    }
    textElement.css('word-wrap', '');
    textElement.css('word-break', '');
    textElement.css('width', '');

    if (width) {
        textElement.css('max-width', width + 1);
    }
}

// Generates a new UUID
function uuid() {
    var d = new Date().getTime();
    var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
        var r = (d + Math.random() * 16) % 16 | 0;
        d = Math.floor(d / 16);
        return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
    });
    return uuid;
}

// Copies text from an input to the clipboard
function copyToClipboard(input) {
    // Un-jquery the input
    input = $(input)[0];

    // Select text
    if (navigator.userAgent.match(/ipad|iphone/i)) {
        var range = document.createRange();
        range.selectNodeContents(input);
        var selection = window.getSelection();
        selection.removeAllRanges();
        selection.addRange(range);
        input.setSelectionRange(0, 999999);
    } else {
        input.select();
    }

    // Copy to clipboard
    document.execCommand('copy');
}

