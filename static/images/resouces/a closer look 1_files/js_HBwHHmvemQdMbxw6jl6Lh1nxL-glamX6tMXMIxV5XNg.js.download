$(function () {
    // Variables ----------------------------------------------------------------------------------

    var dataAttrSlide = "slide";
    var dataAttrNumCards = "num-cards";
    var dataAttrNumSlides = "num-slides";

    var selectorCard = ".hcl__card,.hcl__search-card";

    // Model --------------------------------------------------------------------------------------

    var HorizontalCardListModel = function () {
        var self = this;

        // Variables ----------------------------------------------------------

        self.Cards = ko.observableArray([]);

        // Functions ----------------------------------------------------------

        // MapJsonToCards(json)
        //
        // Maps the given json array to `Card` objects and inserts them into our cards array.
        //
        self.MapJsonToCards = function (json) {
            self.Cards.removeAll();

            $.each(json, function (_, card) {
                self.Cards.push(new Card(card));
            });
        };

        // AfterRender(elements)
        //
        // Knockout callback that is called after knockout has finished rendering a `.hcl` element.
        // The `elements` argument contains an array of all of the children of the `.hcl` element.
        //
        self.AfterRender = function (elements) {
            // Get our HCL element from one of the elements
            var hcl = $(elements).first().parents(".hcl");

            // Count the number of cards in this HCL
            var cards = hcl.find(selectorCard);
            var numCards = cards.length;

            // Calculate the number of "slides" we require (i.e. sets of 4 cards that we show at a time)
            var numSlides = Math.ceil(numCards / 4);

            // Update our HCL element with data attributes that we use for our next/prev buttons
            hcl.data(dataAttrSlide, 0);
            hcl.data(dataAttrNumCards, numCards);
            hcl.data(dataAttrNumSlides, numSlides);

            // Add a dot for each slide
            var dots = hcl.find(".hcl__dots");
            for (var i = 0; i < numSlides; i++) {
                var cardRange = getSlideRange(hcl, i);
                dots.append("<button class=\"hcl__dot-button\" type=\"button\" role=\"presentation\" aria-hidden=\"true\"><span class=\"sr-only\">Go to cards " + cardRange.min + " to " + cardRange.max + "</span></button>");            }

            // Update elements
            updateElementsAfterChangingSlide(hcl);

            // Add save buttons to all our cards (except the search card!)
            cards.filter(".hcl__card").each(function (_, element) {
                var card = $(element);

                ContentCards.addSaveButtonToCard(card);
            });
        };
    };

    // Knockout -----------------------------------------------------------------------------------

    // handleKnockoutBindings()
    //
    // Handles setting up knockout bindings for all horizontal card list elements on the page.
    //
    var handleKnockoutBindings = function () {
        $(".hcl").each(function () {
            // Get JSON
            var jsonString = $(this).find("input[name=cardListJson]").val();
            var json = JSON.parse(jsonString);

            // Build model
            var model = new HorizontalCardListModel();
            model.MapJsonToCards(json);

            // Bind model to this element via knockout
            ko.applyBindings(model, this);
        });
    };

    // Buttons ------------------------------------------------------------------------------------

    // handleNextPrevButtons()
    //
    // Handles next/prev button click events.
    //
    var handleNextPrevButtons = function () {
        // Next button
        $(".hcl__cards-nav-button--next").on("click", function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();

            // Get HCL and go next
            var hcl = $(this).parents(".hcl");
            var slide = hcl.data(dataAttrSlide);

            goToSlide(hcl, slide + 1);
        });

        // Prev button
        $(".hcl__cards-nav-button--prev").on("click", function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();

            // Get HCL and go prev
            var hcl = $(this).parents(".hcl");
            var slide = hcl.data(dataAttrSlide);

            goToSlide(hcl, slide - 1);
        });
    };

    // handleDotButtons()
    //
    // Handles dot button click events.
    //
    var handleDotButtons = function () {
        // Dot button
        $(".hcl__dot-button").on("click", function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();

            // Get HCL and go to this dot's slide
            var hcl = $(this).parents(".hcl");
            var slide = $(this).index();

            goToSlide(hcl, slide);
        });
    };

    // Helpers ------------------------------------------------------------------------------------

    // getSlideRange
    //
    // Work out the range of card numbers for a slide in the HCL. Returns min, max card numbers for a slide number.
    //
    var getSlideRange = function (hcl, slideNumber) {
        var minCard = 0;
        var maxCard = 0;
        var numSlides = hcl.data(dataAttrNumSlides);
        var numCards = hcl.data(dataAttrNumCards);

        if (slideNumber >= 0 && slideNumber <= numSlides) {
            minCard = 1 + (slideNumber * 4);
            maxCard = slideNumber === numSlides - 1 ? numCards : (slideNumber + 1) * 4;
        }

        return {
            min: minCard,
            max: maxCard
        };  
    };

    // goToSlide(hcl, slide)
    //
    // Goes to the next or previous "slide" for the given HCL.
    //
    var goToSlide = function (hcl, newSlide) {
        // Update our current slide number
        hcl.data(dataAttrSlide, newSlide);

        // Update elements
        updateElementsAfterChangingSlide(hcl);

        // Shift to new slide by adjusting the `left` value for all our cards
        var singleCardWidth = 225 + 20; // Width of 225px, with right margin of 20px
        var left = -newSlide * 4 * singleCardWidth;

        hcl.find(selectorCard).css("left", left + "px");
    };

    // updateElementsAfterChangingSlide(hcl)
    //
    // Updates various elements after we have changed slide. E.g. visiblity of next/prev buttons, dots, etc.
    //
    var updateElementsAfterChangingSlide = function (hcl) {
        // Get props
        var slide = hcl.data(dataAttrSlide);
        var numSlides = hcl.data(dataAttrNumSlides);

        // Get next/prev buttons
        var prevButton = hcl.find(".hcl__cards-nav-button--prev");
        var nextButton = hcl.find(".hcl__cards-nav-button--next");
        var prevButtonRangeText = $(prevButton).find(".hcl__cards-nav-button-range");
        var nextButtonRangeText = $(nextButton).find(".hcl__cards-nav-button-range");

        // Clear range text in prev/next buttons
        prevButtonRangeText.text("");
        nextButtonRangeText.text("");

        // Show/hide next/prev buttons
        if (slide === 0) {
            prevButton.addClass("hcl__cards-nav-button--hiding");
            setTimeout(function () {
                prevButton.removeClass("hcl__cards-nav-button--shown");
                prevButton.removeClass("hcl__cards-nav-button--hiding");
            }, 300);
        } else {
            // Populate card range into button text.
            var prevCardRange = getSlideRange(hcl, slide - 1);
            prevButtonRangeText.text(prevCardRange.min + " to " + prevCardRange.max);
            prevButton.addClass("hcl__cards-nav-button--shown");
        }

        if (slide === numSlides - 1) {
            nextButton.addClass("hcl__cards-nav-button--hiding");
            setTimeout(function () {
                nextButton.removeClass("hcl__cards-nav-button--shown");
                nextButton.removeClass("hcl__cards-nav-button--hiding");
            }, 300);
        } else {
            // Populate card range into button text.
            var nextCardRange = getSlideRange(hcl, slide + 1);
            nextButtonRangeText.text(nextCardRange.min + " to " + nextCardRange.max);
            nextButton.addClass("hcl__cards-nav-button--shown");
        }

        // Update dots
        var dots = hcl.find(".hcl__dot-button");

        dots.removeClass("hcl__dot-button--filled");
        $(dots.get(slide)).addClass("hcl__dot-button--filled");
    };

    // Events -------------------------------------------------------------------------------------

    // When the page has loaded
    $(document).ready(function () {
        // Knockout
        handleKnockoutBindings();

        // Buttons
        handleNextPrevButtons();
        handleDotButtons();
    });
});
