// Loads other cards if a user scrolls to the end of the area
// Parameters:
//      loadCardsCallback: Triggered callback toload the cards
//      doNotLoadDelegate: function reference to prevent load (ie. if another load is in progress)
function ContentCardsContainer(loadCardsCallback, doNotLoadDelegate) {
    this.loadCardsCallback = loadCardsCallback;
    this.doNotLoadDelegate = doNotLoadDelegate;

    this.scrollHandler = function (data, e) {
        // Disable on desktop and while loading
        if (isDesktop() || this.doNotLoadDelegate())
            return;

        var containerElement = $(e.target);
        var lastCard = containerElement.children('.content-card').last();
        var lastCardRightOffset = lastCard.offset().left + lastCard.width() - containerElement.offset().left;
        lastCardRightOffset = lastCardRightOffset - 200; // Give some tolerance as it returns fractions

        if (lastCardRightOffset <= containerElement.width() && this.loadCardsCallback !== null) {
            this.loadCardsCallback(containerElement.attr('data-category-name'));
        }
    };
};
var SearchModel = function (filters) {
    // Prepare
    var self = this;

    // List cards of being displayed
    self.CardCategories = ko.observableArray([]);

    // Total number of cards
    self.Total = ko.observable().extend({ notify: 'always' });

    // Total number of cards with text
    self.TotalResources = ko.observable();

    // Current categories being searched
    self.CategoryList = ko.observableArray([]);

    // Card categories with icon URL to be displayed by front-end
    self.CategoryListWithIcon = ko.observableArray([]);

    // Full list of categories available for an initial search
    self.AllCategories = ko.observableArray([]);

    // Text to display when there are no results
    self.NoResultsTexts = ko.observable();

    // Number used to generate a new randomization
    self.Seed = ko.observable();

    // Flag that indicates if a randomization is required
    self.GenerateNewSeed = ko.observable("false");

    // Flag that indicates if a spell check suggestion gets displayed
    self.EnableSuggestion = ko.observable().extend({ notify: 'always' });

    // Spell check suggestion
    self.Suggestion = ko.observable();

    // Original term search (in case the user want to search a mispelled word)
    self.OriginalTerm = ko.observable('');

    // Flag to indicate the backend no to perform a spell check
    self.SpellCheckKeywords = ko.observable(true);

    // List of filters associated with the cards
    self.ApplicableFilters = ko.observableArray([]);

    // Pagination
    self.Pagination = null;

    // Number of cards to show
    self.NumCardsToShow = ko.observable(0);

    // Contains all the values to be sent to the backend to perform a search
    self.FilterModel = ko.observable(filters);
    self.ContentCardsContainerHandler = null;

    // Flags to display loading messages
    self.IsLoadingResults = ko.observable().extend({ notify: 'always' });
    self.IsLoadingMoreCards = ko.observable().extend({ notify: 'always' });
    self.IsReady = ko.observable(false).extend({ notify: 'always' });

    // Tags generated by dst questions
    // TODO: This is legacy, may be able to be deleted
    self.tags = '';

    // Filters generated by DST questions
    self.dstFilters = [];

    // DST free text search text
    self.dstSearchText = '';

    // Whether or not we're on the search page
    self.IsSearchPage = ko.observable(false);

    // Whether or not we're on a topic page
    self.IsTopicPage = ko.observable(false);

    // Whether or not we're on the saved cards page
    self.IsSavedCardsPage = ko.observable(false);

    // Whether or not we're on the share resources page
    self.IsShareResourcesPage = ko.observable(false);

    // Whether or not we're showing a single card (used for emailing single cards)
    self.IsSingleCard = ko.observable(false);

    // Whether or not we're in grid view
    self.IsInGridView = ko.observable(true);

    // Refreshes shown cards using our preloaded cards
    self.RefreshShownCards = function (resetCategoryName, noAnimation) {
        // Get pagination
        var pagination = self.Pagination || {};

        // Determine number of cards to show
        var num = 0;
        var numToShowAllCards = 1000000;
        var noFocus = false;

        if (self.IsShareResourcesPage()) {
            // Share resources page

            // Want to show _all the cards_
            num = numToShowAllCards;
            noFocus = true;
        } else if (self.IsSavedCardsPage()) {
            // Saved cards page

            // Want to show _all the cards_
            num = numToShowAllCards;
            noFocus = true;
        } else if (self.IsTopicPage()) {
            // Topic page
            if (isDesktop()) {
                num = 2;
            } else {
                // Want to show _all the cards_
                num = numToShowAllCards;
            }
        } else if (self.FilterModel().SearchType() === Constants.SearchType.Dst) {
            // DST
            num = pagination.NumShowDST;
        } else if (self.IsInGridView()) {
            // Grid
            if (isDesktop()) {
                num = pagination.NumShowGridDesktop;
            } else if (isTablet()) {
                num = pagination.NumShowGridTablet;
            } else {
                num = pagination.NumShowGridMobile;
            }
        } else {
            // List
            if (isDesktop()) {
                num = pagination.NumShowListDesktop;
            } else if (isTablet()) {
                num = pagination.NumShowListTablet;
            } else {
                num = pagination.NumShowListMobile;
            }
        }

        // Confirm num is valid
        if (num) {
            // Un-observable it if needed
            num = ko.isObservable(num) ? num() : num;

            // Store num
            self.NumCardsToShow(num);

            // Iterate over categories
            self.CardCategories().forEach(function (category, i) {
                // Prepare
                var cards = category.Cards();
                var preloadedCards = category.PreloadedCards();
                var numCards = cards.length;
                var numRequestedCards = category.NumRequestedCards();

                // Special case when we're trying to show all the cards
                if (num === numToShowAllCards) {
                    // Trying to show all the cards
                    numRequestedCards = numToShowAllCards;
                } else {
                    // Not trying to show all the cards

                    // Check if we're showing enough (or too many) cards
                    if (numCards < num) {
                        // Not enough shown - show the rest (up to num) without any animation/focus
                        cards = cards.concat(preloadedCards.slice(0, num - numCards).map(function (card, index) {
                            card.NoAnimation = true;
                            card.NoFocus = true;
                            return card;
                        }));
                        preloadedCards.splice(0, num - numCards);
                    } else if (numCards > num && (!category.HasShownMore() || category.Name() === resetCategoryName)) {
                        // We've got too many (and we haven't already used "show more", or we want to reset) so let's reset back to our expected num
                        preloadedCards = preloadedCards.concat(cards.slice(-(numCards - num)));
                        cards = cards.slice(0, num);
                    }
                }

                // Add newly requested cards
                if (numRequestedCards > 0) {
                    var slice = preloadedCards.slice(0, numRequestedCards);
                    numRequestedCards -= Math.min(slice.length, preloadedCards.length);

                    cards = cards.concat(slice.map(function (card, index) {
                        card.Index = index;
                        card.NoAnimation = noAnimation;
                        card.NoFocus = noFocus;
                        return card;
                    }));
                    preloadedCards.splice(0, slice.length);
                }

                // Save
                category.NumRequestedCards(numRequestedCards);
                category.Cards(cards);
                category.PreloadedCards(preloadedCards);
            });

            // Update cache
            self.StoreCardsInCache();
        }
    };

    // Detect when we change from list to grid view
    self.IsInGridView.subscribe(function () {
        self.RefreshShownCards();
    });

    // Indicates if all of the cards from a category were displayed
    self.HasDisplayedAllCardsFromCategory = function (categoryName) {
        return self.CardCategories().filter(function (category) {
            return category.Name() === categoryName;
        }).some(function (category) {
            return category.Cards().length >= category.TotalCards();
        });
    };

    // Takes the backend response and map that to the KO model
    self.MapJsonToCards = function (jobj) {
        // Clear out our cards
        self.CardCategories.removeAll();

        // Map categories
        self.CategoryListWithIcon(jobj.CategoryTotal);
        self.CategoryList(jobj.CategoryTotal.map(function (category) {
            return category.CategoryName;
        }));
        self.AllCategories(self.CategoryList().slice(0));

        // Handle each distinct category
        self.CategoryList().forEach(function (categoryName) {
            // Get cards for this category
            var categoryCards = jobj.CardList.filter(function (card) {
                return card.CategoryName === categoryName;
            }).map(function (card) {
                return new Card(card);
            });

            // Get total cards for this category
            var totalCardsByCategory = 0;

            jobj.CategoryTotal.filter(function (categoryTotal) {
                return categoryTotal.Category === categoryName;
            }).forEach(function (categoryTotal) {
                totalCardsByCategory = categoryTotal.TotalCards;
            }, 0);

            // Get category icon and description for this category
            var categoryIcon = "";
            var categoryDescription = "";

            self.CategoryListWithIcon().filter(function (category) {
                return category.CategoryName === categoryName;
            }).forEach(function (category) {
                categoryIcon = category.CategoryIconSrc;
                categoryDescription = category.CategoryDescription;
            });

            // Build category
            self.CardCategories.push(new Category(
                categoryName,
                categoryCards,
                jobj.Pagination.CurrentPage,
                jobj.Pagination.PageSize,
                totalCardsByCategory,
                categoryIcon,
                categoryDescription
            ));
        });

        // Refresh shown cards
        self.RefreshShownCards();

        // Spell check
        self.EnableSuggestion(jobj.EnableSuggestion);
        self.Suggestion(jobj.Suggestion);
        self.OriginalTerm(jobj.OriginalTerm);
        self.ApplicableFilters(jobj.ApplicableFilters);

        // Misc
        self.Seed(jobj.Seed);
        self.Total(jobj.Pagination.Total);
        self.TotalResources(self.Total() + " resource" + (self.Total() === 1 ? "" : "s"));
    };

    // Return the card data based on the id from the local model.
    self.GetCardDetails = function (id) {
        // Loop through the cards to find the matching id. If you got a better "id" go for it :).
        // Btw, using jquery loop is cool and "consistent", but is a bit less efficient than classic js, and works with callbacks, that might need more code to bind "this"
        for (var i = 0; i < self.CardCategories(); i++) {
            return self.CardCategories()[i].find(function (c) {
                return c.ItemId === id;
            });
        }

        return null;
    };

    // Cache the current modal
    self.StoreCardsInCache = function () {
        // Only store cards for the service search
        if (self.IsSearchPage()) {
            var cardsData = ko.mapping.toJS(self);

            // A save to cache may happen while these settings are still true, which will disrupt next page visit, so we need to reset them
            cardsData.IsLoadingResults = false;
            cardsData.IsLoadingMoreCards = false;

            // Update cache
            store.session.set(Constants.SessionStorage.CardCache, cardsData);
        }
    }

    // Empty current cache 
    self.ClearCurrentCache = function () {
        store.session.remove(Constants.SessionStorage.CardCache);
    }

    // Return the card data from local storage
    self.GetCacheCardDetails = function () {
        var jobj = store.session.get(Constants.SessionStorage.CardCache);

        // Most of SearchModal properties are observable except for Card array is plain JS array, so we need to tweak it
        var mapping = {
            'CardCategories': {
                create: function (options) {
                    return ko.mapping.fromJS(options.data,
                        {
                            'Cards': {
                                create: function (options) {
                                    return new Card(options.data);
                                }
                            },
                            'PreloadedCards': {
                                create: function (options) {
                                    return new Card(options.data);
                                }
                            }
                        });
                }
            },
            // We don't need to initialize NoResultsTexts as the validation is based on not having a value
            // Prevents an error ([object Object]) when search returns no results
            'NoResultsTexts': {
                create: function (options) {
                    return undefined;
                }
            }
        };

        ko.mapping.fromJS(jobj, mapping, self);

        // If we have no cached card categories, just call GetCards instead
        if (!jobj.CardCategories || jobj.CardCategories.length === 0) {
            self.FilterModel().RemoveFilters();
            self.FilterModel().Keywords("");
            self.GetCards();
        }
    };

    // Ajax call to get cards, used below
    // This has been separated out to make it easier to work with in the SPP project
    self.AjaxGetCards = function (category, showAll) {
        return $.ajax({
            url: "/dmhgapi/ContentCardSearch/CardSearch",
            type: "POST",
            data: ko.toJSON(self.FiltersToJson(category, showAll)),
            contentType: "application/json; charset=utf-8"
        });
    };

    // Gets cards from the server
    self.GetCards = function (currentCategory, showAll) {
        // Set the seed to 0 so it can be generated by backend, "Show More" should not generate new seed
        if (self.GenerateNewSeed() === "true") {
            self.Seed(0);
        }

        // Build DST query
        if (!currentCategory && self.dstFilters.length > 0) {
            self.dstFilters.forEach(function (dstFilter) {
                self.FilterModel().PushFilter(
                    dstFilter.SearchGroupName,
                    dstFilter.Guid,
                    dstFilter.SearchName,
                    true
                );
            });

            self.tags.split('~').forEach(function (tag) {
                var filters = tag.split('|');

                if (filters[0] !== '') {
                    self.FilterModel().PushFilter(filters[0].trim(), filters[1].trim());
                }
            });
        }

        // Check we're using "show more" or "show all" for a category
        if (currentCategory) {
            // Show more/all

            // Mark category as having shown more
            currentCategory.HasShownMore(true);

            // Determine if we've already loaded all the cards
            var alreadyLoadedAllCards = currentCategory.Cards().length + currentCategory.PreloadedCards().length >= currentCategory.TotalCards();

            // Prepare
            var shouldStartNonSilentLoad = false;

            // Check if we're using "show all"
            if (showAll) {
                // Show all

                // Handle already having loaded all cards
                if (alreadyLoadedAllCards) {
                    currentCategory.NumRequestedCards(currentCategory.PreloadedCards().length);
                    self.RefreshShownCards(null, true);

                    self.IsLoadingResults(false);
                    self.IsLoadingMoreCards(false);

                    return;
                }

                // Otherwise, we're going to be loudly loading more cards
                shouldStartNonSilentLoad = true;
            } else {
                // Show more

                // Update our number of requested cards, then try showing 'em
                currentCategory.NumRequestedCards(currentCategory.NumRequestedCards() + self.NumCardsToShow());
                self.RefreshShownCards();

                // Handle already having loaded all cards
                if (alreadyLoadedAllCards) {
                    self.IsLoadingResults(false);
                    self.IsLoadingMoreCards(false);
                    return;
                }

                // Otherwise, we're going to either show more preloaded cards or (pre)load more cards
                shouldStartNonSilentLoad = currentCategory.NumRequestedCards() > 0;
            }

            // Check if we're about to load more cards non-silently (i.e. with a spinner)
            if (shouldStartNonSilentLoad) {
                // Show spinner
                currentCategory.Cards.push(new SpinnerCard(currentCategory.Name()));

                // Show loading
                currentCategory.IsLoading(true);
                self.IsLoadingResults(true);
                self.IsReady(true);
            }

            // Update our page
            currentCategory.CurrentPage(currentCategory.CurrentPage() + 1);
        } else {
            // Not show more

            // Show loading
            self.IsLoadingResults(true);
            self.IsReady(true);
        }

        // Make the actual backend call
        self.AjaxGetCards(currentCategory, showAll).done(function (jsonData) {
            // Success

            // Parse our JSON
            var jobj = $.parseJSON(jsonData);

            // Store pagination data
            self.Pagination = jobj.Pagination;

            // Determine what to do
            if (!currentCategory) {
                // No category (i.e. initial fetch)
                if (jobj.CardList.length === 0) {
                    // No cards

                    // Clear cache
                    self.ClearCurrentCache();

                    // Clear observables
                    self.CardCategories.removeAll();
                    self.Total(0);
                    self.GetNoResultsTexts();

                    // Spell check observables
                    self.EnableSuggestion(jobj.EnableSuggestion);
                    self.Suggestion(jobj.Suggestion);
                    self.OriginalTerm(jobj.OriginalTerm);
                    self.SpellCheckKeywords(true);
                } else {
                    // Got cards

                    // Reset our categories, then map our cards
                    self.CardCategories.removeAll();
                    self.MapJsonToCards(jobj);

                    // Store cards in cache
                    self.StoreCardsInCache();
                }
            } else {
                // Have category (i.e. using "show more")

                // Remove spinner for our category
                currentCategory.Cards(currentCategory.Cards().filter(function (card) {
                    return !(card instanceof SpinnerCard);
                }));
                
                // Load cards
                jobj.CardList.forEach(function (card, index) {
                    currentCategory.PreloadedCards.push(new Card(card, index));
                });

                // Update pagination
                currentCategory.PageSize(jobj.Pagination.PageSize);

                // If we're using "show all", show 'em all
                if (showAll) {
                    currentCategory.NumRequestedCards(currentCategory.PreloadedCards().length);
                }

                // Refresh shown cards
                self.RefreshShownCards(null, showAll);

                // Hide loading
                currentCategory.IsLoading(false);
            }
            
            // Hide loading
            self.IsLoadingResults(false);
            self.IsLoadingMoreCards(false);
        }).fail(function () {
            // Failure

            // Wipe our cards and show an error
            self.CardCategories.removeAll();
            self.Total(0);
            self.GetNoResultsTexts();

            // Hide loading
            if (currentCategory) {
                currentCategory.IsLoading(false);
            }
            
            self.IsLoadingResults(false);
            self.IsLoadingMoreCards(false);
        });
    };

    // Gets initial cards
    self.GetInitialCards = function () {
        // Prepare to get cards
        this.CategoryList.removeAll();
        this.GenerateNewSeed("true");

        // Get cards
        this.GetCards();
    };

    // When the show more/all button on a list of cards is clicked
    self.ShowMore = function (categoryName, showAll, showLess) {
        // Return if there is nothing more to load
        if (self.HasDisplayedAllCardsFromCategory(categoryName)) {
            return;
        }

        // Find our category
        self.CardCategories().filter(function (category) {
            return category.Name() === categoryName;
        }).forEach(function (category) {
            // Get cards
            self.GenerateNewSeed("false");
            self.IsLoadingMoreCards(true);
            self.GetCards(category, showAll);
        });
    };

    // When the show less button on a list of cards is clicked
    self.ShowLess = function (categoryName, button) {
        // Find our category
        self.CardCategories().filter(function (category) {
            return category.Name() === categoryName;
        }).forEach(function (category) {
            // Go back to our minimum num of cards shown
            self.RefreshShownCards(categoryName);

            // Focus our show all button (after a delay)
            var container = $(button).parent();
            setTimeout(function () {
                container.find('.content-card-show-all-button').focus();
            }, 100);
        });
    };

    // Builds JSON representation of our state
    self.FiltersToJson = function (category, showAll) {
        return {
            Keywords: self.FilterModel().Keywords,
            SearchFilterGroups: self.FilterModel().SearchFilterGroups,
            CategoryName: category ? [category.Name()] : [],
            Page: category ? category.CurrentPage() : 0,
            SearchType: self.FilterModel().SearchType(), // Possible values: "search_service", "dst"
            DeviceType: getDeviceType(),
            Seed: self.Seed(),
            SpellCheckKeywords: self.SpellCheckKeywords(),
            ShowAll: showAll
        };
    };

    // Returns whether or not we're currently loading something
    self.IsLoading = function () {
        return self.IsLoadingResults() || self.IsLoadingMoreCards();
    };

    // Returns email data
    self.GetEmailData = function () { 
        var result = {
            CardListEmailInfo: new EmailInformation(
                self.CardCategories(),
                self.FilterModel(),
                self.FilterModel().Keywords,
                self.IsSavedCardsPage(),
                self.IsSingleCard()
            )
        };

        // Special case for DST
        if (self.dstFilters.length > 0) {
            // Remove DST filters from our filters list
            result.CardListEmailInfo.Filters = result.CardListEmailInfo.Filters.filter(function (filterId) {
                return self.dstFilters.map(function (dstFilter) {
                    return dstFilter.Guid;
                }).indexOf(filterId) === -1;
            });

            // Set our keywords to our DST search text
            result.CardListEmailInfo.Keywords = self.dstSearchText;
        }

        return ko.toJSON(result);
    };

    // Ajax call to get our 'no results' text, used below
    // This has been separated out to make it easier to work with in the SPP project
    self.AjaxGetNoResultsTexts = function () {
        return $.ajax({
            url: "/dmhgapi/ContentCardSearch/GetNoResultsTexts",
            type: "GET",
            contentType: "application/json; charset=utf-8"
        });
    };

    // Gets our 'no results' text
    self.GetNoResultsTexts = function () {
        // Check if we have our no results text
        if (self.NoResultsTexts() === undefined) {
            // Don't have it

            // Fetch it
            self.AjaxGetNoResultsTexts().done(function (jsonData) {
                // Save it
                self.NoResultsTexts($.parseJSON(jsonData));

                // Notify that we are done with loading results
                self.IsLoadingResults(false);
            });
        } else {
            // Already have it

            // Notify that we are done with loading results
            self.IsLoadingResults(false);
        }
    };

    // Clears our filters
    self.ClearCheckbox = function () {
        self.FilterModel().SearchFilterGroups().forEach(function (searchFilterGroup) {
            searchFilterGroup.Filters.removeAll();
            searchFilterGroup.FilterIds.removeAll();
        });

        self.FilterModel().FilterName.removeAll();
        self.FilterModel().EmailFilters.removeAll();
    };

    // When a 'search instead' link is clicked (from the auto correct feature)
    self.SearchInstead = function () {
        self.FilterModel().Keywords(self.OriginalTerm());
        self.SpellCheckKeywords(false);
        self.CategoryList.removeAll();
        self.GetCards();
    };

    // Returns the number of topic cards to show via 'show more'
    self.TopicCardShowMoreCount = function () {
        return getDeviceType() === Constants.Device.Desktop ? 2 : 4;
    };

    // Returns ture if an element was already bound
    self.isBound = function (id) {
        return !!ko.dataFor(document.getElementById(id));
    };

    // Initialize page
    self.ContentCardsContainerHandler = new ContentCardsContainer(self.ShowMore, self.IsLoading.bind(self));

    // When the window is resized
    $(window).on('resize', function (e) {
        self.RefreshShownCards();
    });
};

// Model used for specifying what to include in a card-related email
var EmailInformation = function (cardList, filters, keywords, isSavedCards, isSingleCard) {
    var self = this;

    self.CardList = [];
    self.Filters = [];

    self.Keywords = keywords;

    for (var i = 0; i < cardList.length; i++) {
        for (var j = 0; j < cardList[i].Cards().length; j++) {
            self.CardList.push(cardList[i].Cards()[j].ItemId);
        }
    }
    if (filters != null) {
        for (var i = 0; i < filters.SearchFilterGroups().length; ++i) {
            for (var j = 0; j < filters.SearchFilterGroups()[i].FilterIds().length; ++j) {
                self.Filters.push(filters.SearchFilterGroups()[i].FilterIds()[j]);
            }
        }
    }

    self.IsSavedCards = isSavedCards;
    self.IsSingleCard = isSingleCard;
}

// Model for each set of cards (one set of cards per category)
var Category = function (categoryName, card, currentPage, pageSize, totalCards, categoryIcon, categoryDescription) {
    var self = this;

    self.Name = ko.observable(categoryName);
    self.Icon = ko.observable(categoryIcon);
    self.Description = ko.observable(categoryDescription);
    self.Cards = ko.observableArray([]);
    self.PreloadedCards = ko.observableArray([]);
    self.CurrentPage = ko.observable(currentPage);
    self.PageSize = ko.observable(pageSize);
    self.TotalCards = ko.observable(totalCards);
    self.NumRequestedCards = ko.observable(0);
    self.IsLoading = ko.observable(false);
    self.HasShownMore = ko.observable(false);

    self.Title = ko.computed(function () {
        return self.Name() + " (" + self.TotalCards() + ")";
    });

    self.NameSlug = ko.computed(function () {
        return slugify(self.Name());
    });

    for (var i = 0; i < card.length; i++) {
        self.PreloadedCards.push(card[i]);
    }

    self.ShowMoreVisible = ko.computed(function () {
        return self.Cards().length < self.TotalCards();
    });
};

// Special card for showing our loading spinner
var SpinnerCard = function (category) {
    var self = this;

    self.TemplateName = "SpinnerContentCard";
    self.CategoryName = category;
};

// Model for the filters that are going to create the query against the index
var FilterModel = function (keywords, searchType) {
    var self = this;

    self.Keywords = ko.observable(keywords);
    self.SearchFilterGroups = ko.observableArray([]);
    self.SearchType = ko.observable(searchType);
    self.FilterName = ko.observableArray([]); // For analytics
    self.EmailFilters = ko.observableArray([]); // For email

    self.PushFilter = function (filterType, filterValue, filterName, isDSTFilter) {
        // Try to find matching search filter group
        var searchFilterGroup = self.SearchFilterGroups().filter(function (searchFilterGroup) {
            return searchFilterGroup.FilterType().toLowerCase() === filterType.toLowerCase();
        }).reduce(function (nil, searchFilterGroup) {
            return searchFilterGroup;
        }, null);

        // If we didn't find one, create (and add) it
        if (!searchFilterGroup) {
            searchFilterGroup = {
                FilterType: ko.observable(filterType),
                Filters: ko.observableArray([]),
                FilterIds: ko.observableArray([])
            };
            self.SearchFilterGroups().push(searchFilterGroup);
        }

        // Try to find matching email filter
        var emailFilter = self.EmailFilters().filter(function (emailFilter) {
            return emailFilter.FilterType === filterType && emailFilter.FilterName === filterName;
        }).reduce(function (nil, emailFilter) {
            return emailFilter;
        }, null);

        // If we didn't find one, create it
        if (!emailFilter) {
            emailFilter = {
                FilterType: filterType,
                FilterName: filterName
            };
        }

        // Check if the search filter group already has our filter
        if (searchFilterGroup.FilterIds().filter(function (filter) {
            return filter === filterValue;
        }).length) {
            // Already added

            // Remove the filter (if it's not a DST filter)
            if (!isDSTFilter) {
                searchFilterGroup.FilterIds.remove(filterValue);
                searchFilterGroup.Filters.remove(filterName);

                self.FilterName.remove(filterName);
                self.EmailFilters.remove(emailFilter);

                if (searchFilterGroup.FilterIds().length === 0) {
                    self.SearchFilterGroups.remove(searchFilterGroup);
                }
            }
        } else {
            // Not already added

            // Add the filter
            searchFilterGroup.FilterIds.push(filterValue);
            searchFilterGroup.Filters.push(filterName);

            // Also add to our other properties if it's not a DST filter
            // (as we don't want to show DST filters in the filter user interface for example)
            if (!isDSTFilter) {
                self.FilterName.push(filterName);
                self.EmailFilters.push(emailFilter);
            }
        }

        // Knockout needs the return true when handling checkbox from a function
        return true;
    };

    self.RemoveFilters = function () {
        for (var i = 0; i < self.SearchFilterGroups().length; i++) {
            var searchFilterGroup = self.SearchFilterGroups()[i];

            searchFilterGroup.FilterIds.removeAll();
            searchFilterGroup.Filters.removeAll();
        }
    };
};

