// Returns string replacing special characters by a hyphen
function slugify(txt) {
    if (txt != undefined && txt != null) {
        const a = '/_,:;'
        const b = 'aaaaaaaaeeeeeiiiioooooouuuunlccsyoarsnpwgnmuxzhzz------'
        const p = new RegExp(a.split('').join('|'), 'g')

        return txt.toString().toLowerCase().trim()
            .replace(/\s+/g, '-')           // Replace spaces with -
            .replace(p, function (c) { return b.charAt(a.indexOf(c)) }) // Replace special chars (verbose function syntax, cause IE11 does not support arrow functions)
            .replace(/&/g, '-and-')         // Replace & with 'and'
            .replace(/[^\w\-]+/g, '')       // Remove all non-word chars
            .replace(/\-\-+/g, '-')         // Replace multiple - with single -
            .replace(/^-+/, '')             // Trim - from start of text
            .replace(/-+$/, '');            // Trim - from end of text
    } else {
        return "";
    }
}

// Returns a query string parameter's value
function getUrlParameter(name) {
    name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
    var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
    var results = regex.exec(location.search);
    return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
}

