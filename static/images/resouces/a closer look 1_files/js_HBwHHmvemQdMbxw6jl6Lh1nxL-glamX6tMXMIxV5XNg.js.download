// Simple character limit handler
function CharacterLimitHandler(limit, input, countContainer, errorContainer) {
    // Variables
    this.limit = limit;
    this.input = input;
    this.countContainer = countContainer;
    this.errorContainer = errorContainer;

    this.inputHandler = null;
    this.countSrElement = null;
    this.countElement = null;
    this.errorElement = null;

    // Initialise
    this.init = function () {
        // If we have no limit, ignore
        if (!this.limit) {
            return;
        }

        // Create DOM elements and update them
        if (this.countContainer) {
            this.countSrElement = $('<div />').appendTo(this.countContainer);
            this.countSrElement.addClass('sr-only');

            this.countElement = $('<div />').appendTo(this.countContainer);
            this.countElement.addClass('character-limit-count');
            this.countElement.attr('aria-hidden', true);
        }

        this.updateDomCountElements();
        this.updateDomErrorElement();

        // Apply bindings
        this.inputHandler = $.proxy(this.inputEventHandler, this);
        this.input.on('input change keyup focus', this.inputHandler);
    };

    // Deinitialise
    this.deinit = function () {
        if (this.countSrElement) {
            this.countSrElement.remove();
        }

        if (this.countElement) {
            this.countElement.remove();
        }

        if (this.errorElement) {
            this.errorElement.remove();
        }

        if (this.input) {
            this.input.off('input change keyup focus', this.inputHandler);
        }
    };

    // Handle input being changed
    this.inputEventHandler = function () {
        this.updateDomCountElements();
    };

    // Gets our current count
    this.getCount = function () {
        return this.input.val().length;
    };

    // Gets whether or not our count is over the limit
    this.isCountOverLimit = function () {
        return this.getCount() > this.limit;
    };

    // Updates the DOM count elements
    this.updateDomCountElements = function () {
        // If we have no limit, ignore
        if (!this.limit) {
            return;
        }

        // If we have no count container, ignore
        if (!this.countContainer) {
            return;
        }
        
        // Update elements
        var count = this.getCount();
        this.countSrElement.html('You have used ' + count + ' characters. There is a limit of ' + limit + ' characters.');
        this.countElement.html(count + ' / ' + limit);
        if (this.isCountOverLimit()) {
            this.countElement.addClass('invalid');
        } else {
            this.countElement.removeClass('invalid');
        }
    };

    // Updates the DOM error element
    this.updateDomErrorElement = function () {
        // If we have no limit, ignore
        if (!this.limit) {
            return;
        }

        // If we have no error container, ignore
        if (!this.errorContainer) {
            return;
        }

        // Update elements
        if (this.isCountOverLimit()) {
            if (this.errorElement) {
                this.errorElement.remove();
            }
            this.errorElement = $('<div />').appendTo(this.errorContainer);
            this.errorElement.addClass('character-limit-error');
            this.errorElement.attr('role', 'alert');
            this.errorElement.attr('aria-atomic', 'true');
            this.errorElement.html('Your response is over by ' + (this.getCount() - limit) + ' characters. Please reduce your response.');

            var that = this;
            setTimeout(function () {
                that.input.focus();
            }, 500);
        } else {
            if (this.errorElement) {
                this.errorElement.css({ display: "none" });
                this.errorElement.remove();
            }
        }
    };

    // Validates the current count and returns whether or not it's within our limit, and displays a validation error if it's not
    this.validate = function () {
        // If we have no limit, ignore
        if (!limit) {
            return true;
        }

        // Update elements
        this.updateDomCountElements();
        this.updateDomErrorElement();

        // Return whether we're validate
        return !this.isCountOverLimit();
    };
}
