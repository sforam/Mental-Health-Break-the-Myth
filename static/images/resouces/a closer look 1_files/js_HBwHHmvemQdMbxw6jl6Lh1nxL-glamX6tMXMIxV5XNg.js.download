// Returns whether or not we're in desktop mode
function isLargeDesktop() {
    return $(window).outerWidth() >= Constants.ScreenWidth.LargeDesktop;
}

// Returns whether or not we're in desktop mode
function isDesktop() {
    return $(window).outerWidth() >= Constants.ScreenWidth.Desktop;
}

// Returns whether or not we're in tablet or mobile mode
function isTabletOrMobile() {
    return $(window).outerWidth() < Constants.ScreenWidth.Desktop;
}

// Returns whether or not we're in mobile mode
function isMobile() {
    return $(window).outerWidth() <= Constants.ScreenWidth.Mobile;
}

// Returns whether or not we're in big mobile mode
function isBigMobile() {
    return $(window).outerWidth() <= Constants.ScreenWidth.BigMobile;
}

// Returns whether or not we're in tablet mode
function isTablet() {
    return isTabletOrMobile() && !isMobile();
}

// Returns whether or not we're in tablet mode
function isSmallTablet() {
    return $(window).outerWidth() <= Constants.ScreenWidth.SmallTablet;
}

// Returns whether or not we're in mobile safari
function isMobileSafari() {
    return /iP(ad|hone|od).+Version\/[\d\.]+.*Safari/i.test(navigator.userAgent);
}

// Returns whether or not we're on a Mac
function isMac() {
    return /^.*?Mac.*?$/i.test(navigator.platform);
}

// Returns whether or not we're on Safari
function isSafari() {
    return /^.*?Safari.*?$/i.test(navigator.userAgent);
}

// Returns whether or not we're on Safari on a Mac
function isDesktopSafari() {
    return isMac() && isSafari();
}

// Returns the type of device
function getDeviceType() {
    if (isDesktop()) {
        return Constants.Device.Desktop;
    }

    if (isTablet()) {
        return Constants.Device.Tablet;
    }

    if (isMobile()) {
