            .append('<span>Feedback bar has been dismissed</span>');
    });

    // When someone clicks the feedback tab open button
    $(document).on('click', '.feedback-tab-open-button', function () {
        // Preselect feedback type radio
        Modals.getFeedbackModal().find('.feedback-modal-feedback-type-' + $(this).attr('data-feedback-type')).click();

        // Setup modal
        setupModal($(this).get(0));

        // Show modal
        Modals.getFeedbackModal().modal('show');
    });

    // When someone clicks the feedback modal next button
    $(document).on('click', '#feedback-modal-next-button', function () {
        // Validate
        if (validateCurrentQuestion()) {
            // Go next
            shiftQuestionIndex(+1);
        }
    });

    // When someone press enter Button to select a field and blocks the form submission
    $(document).on('keypress', 'form input', function (e) {
        var keyCode = e.keyCode || e.which;
        if (keyCode === Constants.KeyCode.Enter &&
            !$(e.target).is('input[type="submit"],input[type="button"]')) {
            e.preventDefault();
            if ($(e.target).is('input[type="checkbox"]')) {
                $(this).prop('checked', $(this).is(':checked') ? false : true);

            } else if ($(e.target).is('input[type="radio"]')) {
                $(this).click();
            }
            return false;
        }
    });

    // When someone clicks the feedback modal back button
    $(document).on('click', '#feedback-modal-back-button', function () {
        // Go back
        shiftQuestionIndex(-1);
    });

    // it clears the error message when someone inputs a wrong captcha and after clicks back on the input to try again the captcha.
    $(document).on('click', '.feedback-modal-captcha', function () {
        var feedbackModalErrorMsg = Modals.getFeedbackModal().find('#feedback-modal-error');
        if (feedbackModalErrorMsg.text().length > 0) {
            feedbackModalErrorMsg.text('');
        }
    });

    // When someone clicks the site wide radio button
    $(document).on('click', '.feedback-modal-feedback-type-page', function (e) {
        // Clear options for page specific feedback
        $('.question.feedback-type-page > fieldset > .checkboxes .Options').prop('checked', false);
        $('.question.feedback-type-page > fieldset > .image-rating .Options').prop('checked', false);
        $('.question.feedback-type-page > fieldset > .number-rating .Options').prop('checked', false);
        $('.question.feedback-type-page > fieldset > .checkboxes textarea').val('');
        $('.question.feedback-type-page > fieldset > .captcha > .feedback-modal-captcha').val('');
    });

    // When someone clicks the page specific radio button
    $(document).on('click', '.feedback-modal-feedback-type-site', function (e) {
        // Clear options for site wide feedback
        $('.question.feedback-type-site > fieldset > .checkboxes .Options').prop('checked', false);
        $('.question.feedback-type-site > fieldset > .image-rating .Options').prop('checked', false);
        $('.question.feedback-type-site > fieldset > .number-rating .Options').prop('checked', false);
        $('.question.feedback-type-site > fieldset > .checkboxes textarea').val('');
        $('.question.feedback-type-site > fieldset > .captcha > .feedback-modal-captcha').val('');
    });

    // When someone clicks the feedback modal submit button
    $(document).on('click', '.feedback-modal-submit-button', function (e) {
        e.preventDefault();

        // Validate
        if (validateCurrentQuestion()) {
            // Go to loading state
            setState(State.Loading, true);

            // Build items
            var items = {};
            Modals.getFeedbackModal().find('.question.feedback-type-' + feedbackType + ' > fieldset').each(function () {
                // Get question type
                var questionType = $(this).find('.QuestionType').val();
                if (questionType) {
                    // Get values
                    var questionId = $(this).find('.QuestionId').val();
                    var options = $(this).find('.Options[type=hidden], .Options:checked').get().map(function (option) {
                        return $(option).val();
                    });
                    var text = $(this).find('.Text').val();
