// Sends session storage to other tabs (called when we update session storage with data that needs to be sync'd)
function sendSessionStorageToOtherTabs() {
    var data = {};
    Constants.SessionStorageSyncKeys.forEach(function (key) {
        data[key] = store.session.get(key);
    });

    store.set(Constants.LocalStorage.SetSessionStorage, data);
    store.remove(Constants.LocalStorage.SetSessionStorage);
}

// Requests session storage from other tabs (called when the page loads)
function requestSessionStorageFromOtherTabs() {
    requestSessionStorageFromOtherTabs.uuid = uuid();

    store.set(Constants.LocalStorage.GetSessionStorage, requestSessionStorageFromOtherTabs.uuid);
    store.remove(Constants.LocalStorage.GetSessionStorage);
}

// Events #####################################################################

// When something is stored in local storage
$(window).on('storage', function (e) {
    // Handle passing session storage between tabs via local storage
    // See: https://stackoverflow.com/a/32766809
    if (e.key === Constants.LocalStorage.GetSessionStorage) {
        // Get request - send set response
        if (e.originalEvent.newValue && e.originalEvent.newValue != requestSessionStorageFromOtherTabs.uuid) {
            sendSessionStorageToOtherTabs();
        }
    } else if (e.key === Constants.LocalStorage.SetSessionStorage) {
        // Set request - update session storage
        if (e.originalEvent.newValue) {
            var data = JSON.parse(e.originalEvent.newValue);
            for (var key in data) {
                store.session.set(key, data[key]);
            }
        }
    }
});

