            .done($.proxy(function(responseData) {
                this.submitButtonLoader.addClass('stopped');

                if (responseData.EmailSent) {
                    this.showEmailSent();
                    this.captchaHandler.load($.proxy(this.displayNewQuestion, this));
                } else {
                    if (responseData.Message !== '') {
                        this.showEmailError(responseData.Message, true);
                        this.displayNewQuestion(responseData.Question);
                    } else {
                        if (responseData.Question !== '') {
                            this.showEmailError('Incorrect answer. Please enter new Captcha code.', true);
                            this.displayNewQuestion(responseData.Question);
                            this.captchaInput.focus();
                        }

                    }
                        
                }
            },
            this));
    }
