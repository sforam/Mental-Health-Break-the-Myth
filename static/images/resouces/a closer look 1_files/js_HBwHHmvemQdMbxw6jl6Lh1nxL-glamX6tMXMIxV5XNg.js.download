// When a video's play or close button are clicked
$(document).on('click', '.youtube-video .play-video-button, .youtube-video .player, .youtube-video .close-video-button', function (e) {
    e.preventDefault();
    e.stopImmediatePropagation();

    // Ignore clicking player on non-mobile
    if ($(this).is('.player') && !isMobile()) {
        return;
    }

    // Prepare
    var video = $(this).parents('.youtube-video');
    var videoParent = video.parent();
    var videoPlaceholder = video.find('.placeholder');
    var videoPlayer = video.find('.player');
    var playVideoButton = video.find('.play-video-button');

    // Special case: use a different parent if we're inside a text image item banner
    var altParent = video.parents('.text-image-item-banner');
    if (altParent.length) {
        videoParent = altParent;
    }

    // Special case: use a different parent if we're on the homepage
    altParent = video.parents('.homepage__video-area');
    if (altParent.length) {
        videoParent = altParent;
    }

    // Add class to our parent (so that we can style it)
    videoParent.addClass('youtube-video-parent');

    // Insert our video HTML
    videoPlaceholder.replaceWith(video.data('video-html'));

    // Toggle open/close (after a slight delay so that our youtube-video-parent class has time to be applied)
    setTimeout(function () {
        var elements = $().add(video).add(videoParent);
        video.off('animationend');
        video.off('touchmove');

        if (video.is('.open')) {
            // Close
            elements.addClass('closing');

            video.on('animationend', function () {
                elements.removeClass('open');
                elements.removeClass('closing');

                // Remove our video HTML
                video.find('iframe').replaceWith('<div class="placeholder"></div>');
            });

            // Re-enable tabbing to elements that we previously disabled tabbing for
            // (see below code "mobile vs desktop")
            accUntrapTabbing();
            $('.youtube-disable-tabbing-container').removeAttr('aria-hidden');

            // Accessibility stuff
            videoPlayer.attr('aria-hidden', true);
            playVideoButton.attr('aria-expanded', false);
            playVideoButton.focus();
        } else {
            // Open
            elements.addClass('open');

            // Prevent scrolling on mobile
            video.on('touchmove', function (e) {
                if (isMobile()) {
                    e.preventDefault();
                }
            }, false);

            // Focus the video
            videoPlayer.attr('tabindex', -1).focus();

            // Handle mobile vs desktop
            if (isMobile()) {
                // Mobile

                // Our video overlay is like a modal, so we want to stop people tabbing out of it
                accTrapTabbing(video);
            } else {
                // Desktop/tablet

                // Our video overlay isn't like a modal, so we only want to stop people tabbing
                // to things directly behind our overlay
                $('.youtube-disable-tabbing-container').attr('aria-hidden', true);
            }

            // Accessibility stuff
            videoPlayer.removeAttr('aria-hidden');
            playVideoButton.attr('aria-expanded', true);
        }
    }, 20);
});

// When a video's view-transcript button is clicked
$(document).on('click', '.youtube-video .view-transcript', function (e) {
